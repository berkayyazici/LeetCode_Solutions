class Solution {
    public boolean isPerfectSquare(int num) {
        
        if(num > 0){
            
            double value = Math.sqrt(num);
            
            if(value == (int) value){
                return true;
            }
            
            else{
                return false;
            }
        }
        
        else{
            return false;
        }
        
    }
}
