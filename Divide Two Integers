class Solution {
    public int divide(int dividend, int divisor) {
        
        int result = 0;
        
        try{
            result = dividend / divisor;    
        }
        
        catch(Exception e){
            System.out.println(e);
        }
        
        
        if((dividend < -2147483647)&&(divisor == -1)){
            
            result = 2147483647;
        }
          
        return result;
             
    }
}
